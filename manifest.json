{
    "applications": {
      "gecko": {
        "strict_min_version": "58.0a1"
      }
    },
    "icons": {
      "48": "potatoIcon48.png",
      "96": "potatoIcon96.png",
      "128": "potatoIcon128.png"
    },
    "browser_action": {
      "browser_style": true,
      "default_title": "idle",
      "default_popup": "idle.html",
      "default_icon": {
        "128": "potatoIcon128.png"
      }
    },
    "background": {
      "scripts": ["background.js"]
    },
    "description": "simple idle clicker u got nothing to do here",
    "homepage_url": "https://github.com/blurpek",
    "manifest_version": 2,
    "name": "idle",
    "permissions": [
      "storage"
    ],
    "version": "1.1"
  }